{
  "name": "amp-log-lib",
  "version": "0.0.2",
  "description": "common log lib for AMP",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./dist && cp -rv src ./dist && babel src -d ./dist --source-maps",
    "test": "mocha --recursive dist/test",
    "cover": "npm run build && isparta cover --root dist/test --report html mocha -- --recursive dist/test/**/*.js",
    "lint": "eslint src",
    "format": "esformatter -i 'src/**/*.js'",
    "watch": "parallelshell 'npm run watch:test -s' 'npm run watch:build -s'",
    "watch:test": "npm run test -- -w",
    "watch:build": "npm run build -- -w"
  },
  "main": "dist",
  "dependencies": {
    "atomiq": "^1.0.7",
    "bunyan": "^1.8.1",
    "debug": "^2.2.0",
    "node-fetch": "^1.3.3",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-register": "^6.9.0",
    "esformatter": "^0.9.3",
    "eslint": "^2.10.2",
    "eslint-plugin-babel": "^3.2.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "node-inspector": "^0.12.7",
    "parallelshell": "^2.0.0",
    "source-map-support": "^0.4.0"
  }
}
